.tab-h {
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    /* overflow: hidden; */
    line-height: 80rpx;
    background: #f7f7f7;
    font-size: 16px;
    white-space: nowrap;
    position: relative;
    top: 0;
    left: 0;
    z-index: 99;
}

.tab-item {
    width: 25%;
    text-align: center;
    display: inline-block;
}

.tab-item.active {
    color: #000;
    position: relative;
}

.tab-item.active:after {
    content: "";
    display: block;
    height: 4rpx;
    width: 100%;
    background: rgb(249, 70, 70);
    position: absolute;
    bottom: 0;
    left:0;
    border-radius: 16rpx;
}

