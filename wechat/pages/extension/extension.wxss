.swiper-view{
margin: 0 auto!important;
display: flex;
padding:40rpx 0;
justify-content:center;
}
swiper {
  height: 575rpx;
}
.da{
  animation:myfirst 0.8s both;
}

@keyframes myfirst
{
from {transform: scale(1);}
to {transform: scale(1.25);}
}

.wfr-swiper{
  width: 85%;
  height:325rpx;
  margin:4rpx auto;
  overflow:hidden;
  text-align:center;
  background-size:100% 100%;
  position:absolute;
  /* filter:blur(10rpx); */
  box-shadow:0px 14px 30px 0px rgba(88,119,255,0.45);
  border-radius:20rpx; 
  background:rgba(255,255,255,0.43);
  /* background:linear-gradient(45deg,rgba(48,46,46,1) 15%,transparent 15%,transparent 50%,rgba(48,46,46,1) 50%,rgba(48,46,46,1) 65%,transparent 65%);
  background-size:16rpx 16rpx; 
  background-color:rgba(43,39,37,1);
  box-shadow:0px 14px 30px 0px rgba(88,119,255,0.45);
  border-radius:20px; */
}
swiper-item{
  overflow: visible !important;
}

.swiper-view:last-child .wfr-swiper{
  background:linear-gradient(45deg,rgba(48,46,46,1) 15%,transparent 15%,transparent 50%,rgba(48,46,46,1) 50%,rgba(48,46,46,1) 65%,transparent 65%); 
  background-size:16rpx 16rpx; 
  background-color:rgba(43,39,37,1); 
  background-position: right;
}
