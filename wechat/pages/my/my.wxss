/* 内容 */
.order{
    height:212rpx;
    background:rgba(255,255,255,1);
    border-radius:6rpx;
    display:flex;
    text-align: center;
    align-items: center;
    justify-content: space-between;
    
}
 .orderList{
     position: relative;
    width:20%;
    height:212rpx;
    box-sizing:border-box;
}
.on{
    height:72rpx;
    width:72rpx;
    padding-bottom:16rpx;
    margin-top:54rpx;
    /* border-right:1px solid lightgray; */
}

.bd:after{
    content:'';
    display: block;
    position: absolute;
    top:50rpx;right:0;
    width:1px;
    height:114rpx;
    background-color:#EDEDED;
}

.orderList .name{
    font-size:24rpx;
    font-family:PingFang-SC-Medium;
    color:rgba(74,74,74,1);
}
.my{
    padding-bottom:108rpx;
    border-top:14rpx solid #EFEFEF;
    border-bottom:12rpx solid #EFEFEF;
}
.myList{
    height:86rpx;
    background:rgba(255,255,255,1);
    box-shadow:0px -2rpx 0rpx 0rpx rgba(237,237,237,1);
    /* border:1rpx solid rgba(237,237,237,1); */
    border-radius:8rpx;
    display:flex;
    align-items: center;
    
}
.myList:nth-child(8){
    border-top:8rpx solid #EFEFEF;
}
.my_img{
    width:48rpx;
    height:48rpx;
    padding:0 32rpx;
}
.red{
    width:48rpx;
    height:48rpx;
    padding:0 32rpx;
}
.text{
    font-size:28rpx;
    /* font-family:PingFang-SC-Medium; */
    color:rgba(59,59,59,1);
    line-height:40rpx;
}