/* pages/my/my.wxss */

/* 头部 */

page {
    width: 100%;
    height: 100%;
    background: #efefef;
}

.head {
    width: 100%;
    height: 315rpx;
    /* background:rgba(64,64,64,0.65); *//* background:url('../../image/view.png'); */
}

image {
    width: 100%;
    height: 100%;
}

.view_img {
    width: 100%;
    height: 315rpx;
    position: absolute;
    z-index: -1;
    float: left;
}

.oimg {
    width: 100%;
    height: 100%;
}

.nav {
    position: relative;
    z-index: 1;
    display: flex;
    align-items: center;
    height: 110rpx;
    top: 160rpx;
}

.scan {
    margin: 16rpx 0;
    background-color: #fff;
}

.scan-title {
    color: #3b3b3b;
    line-height: 86rpx;
    font-size: 28rpx;
    text-align: center;
    box-shadow: 0 2rpx #ededed;
}

.scan-content {
    padding: 28rpx 44rpx 32rpx 32rpx;
}

.con-title {
    font-size: 28rpx;
    color: #9b9b9b;
    padding-bottom: 24rpx;
}

.flex {
    display: flex;
    justify-content: space-between;
}

.number {
    color: #9b9b9b;
    font-size: 28rpx;
}

.number text {
    color: 28rpx;
}

.teamtwo{
    margin-bottom:34rpx;
    
}
progress{
    flex:1;
    margin-right:12rpx;
}
.dabiao{
     width: 84rpx;
    padding: 0 20rpx;
    height: 40rpx;
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    line-height: 40rpx;
    border-radius: 16rpx;
}
.green {
    color: rgba(8, 194, 153, 1);
    border: 2rpx solid rgba(8, 194, 153, 1);
}

.red{
    color: #D0021B;
    border:#D0021B
}
