.logo {
    width: 110rpx;
    height: 110rpx;
    margin: 0 48rpx;
}

.logo image {
    height: 100%;
    width: 100%;
    border-radius: 50%;
    border: 2px solid #fff;
}

.meimei {
    /* width:218rpx; */
    height: 34rpx;
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    color: rgba(255, 255, 255, 1);
    line-height: 34rpx;
    margin-bottom: 12rpx;
}

.tuan {
    display: flex;
    justify-content: baseline;
    align-items: center;
    width: 440rpx;
}

.tuan image {
    width: 48rpx;
    height: 48rpx;
}

.alex {
    margin: 0 20rpx;
    font-size: 32rpx;
    font-family: SFUIText-Medium;
    color: rgba(255, 255, 255, 1);
}

.tuanzhang {
    width: 48rpx;
    height: 34rpx;
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    color: rgba(255, 255, 255, 1);
    line-height: 34rpx;
    border: 1px solid lightgray;
    padding: 0 14rpx;
    text-align: center;
    border-radius: 15rpx;
}

.setting {
    width: 48rpx;
    height: 48rpx;
    position: relative;
    z-index: 10;
}

.settingimage {
    width: 100%;
    height: 100%;
}

/* 内容 */

.order {
    height: 212rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 6rpx;
    display: flex;
    text-align: center;
    align-items: center;
    justify-content: space-between;
}

.orderList {
    position: relative;
    width: 20%;
    height: 212rpx;
    box-sizing: border-box;
}

.on {
    height: 72rpx;
    width: 72rpx;
    padding-bottom: 16rpx;
    margin-top: 54rpx;
    /* border-right:1px solid lightgray; */
}

.bd:after {
    content: '';
    display: block;
    position: absolute;
    top: 50rpx;
    right: 0;
    width: 1px;
    height: 114rpx;
    background-color: #ededed;
}

.orderList .name {
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    color: rgba(74, 74, 74, 1);
}

.my {
    padding-bottom: 180rpx;
    /* border-top: 14rpx solid #efefef; */
    border-bottom: 28rpx solid #efefef;
}

.myList {
    height: 86rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -2rpx 0rpx 0rpx rgba(237, 237, 237, 1);
    /* border:1rpx solid rgba(237,237,237,1); */
    border-radius: 8rpx;
    display: flex;
    align-items: center;
}

.myList:nth-child(8) {
    border-top: 24rpx solid #efefef;
}

.my_img {
    width: 48rpx;
    height: 48rpx;
    padding: 0 32rpx;
}

.red {
    width: 48rpx;
    height: 48rpx;
    padding: 0 32rpx;
}

.text {
    font-size: 28rpx;
    /* font-family:PingFang-SC-Medium; */
    color: rgba(59, 59, 59, 1);
    line-height: 40rpx;
}
