.donghua{
    animation:kkk 0.2s linear forwards;   
}
@keyframes kkk{
    0%{
        transform:rotateZ(0deg);
    }
    100%{
        transform:rotateZ(90deg);
    }
}
.donghua1{
    animation:kkk1 0.2s linear forwards;   
}
@keyframes kkk1{
    0%{
        transform:rotateZ(90deg);
    }
    100%{
        transform:rotateZ(0deg);
    }
}
