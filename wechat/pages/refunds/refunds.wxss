.select_box {
    background-color: #eee;
    padding: 0 10rpx;
    width: 90%;
    position: absolute;
    top: 50rpx;
    z-index: 1;
    overflow: hidden;
    animation: myfirst 0.5s;
    margin: 0 auto;
}

@keyframes myfirst {
    from {
        height: 0rpx;
    }

    to {
        height: 100%;
    }
}

