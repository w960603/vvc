.page {
    width: 100%;
    /* min-height:100%; */
}
.userinfo-btn{
    position:fixed;
    top:0;left:0;
    z-index:999;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0,0,0,0);
}

.header {
    display: flex;
    align-items: center;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    position: relative;
}

