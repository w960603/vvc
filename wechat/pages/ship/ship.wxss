.header {
    position: fixed;
    top:0;left:0;
    width: 100%;
    height: 88rpx;
    background: rgba(246, 246, 246, 1);
    z-index: 99;
    line-height: 88rpx;
    text-align: center;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 40rpx;
}

.header .back {
    height: 48rpx;
    width: 48rpx;
    padding: 0 32rpx;
    
}
::-webkit-scrollbar{
    display: none
}
.white {
    opacity: 0;
}

image {
    width: 100%;
    height: 100%;
}

.content {
    width: 100%;
    background: rgba(255, 255, 255, 1);
    z-index: 99;
}

/* 导航 */

.nav {
    width: 100%;
    height: 434rpx;
    background: rgba(255, 255, 255, 1);
    border-bottom: 16rpx solid rgba(246, 246, 246, 1);
}

.ship_back {
    width: 704rpx;
    height: 402rpx;
    margin: 0 auto;
    position: relative;
    left: 0;
    top: 0;
}

.ship_back .red_img {
    position: absolute;
    left: 0;
    top: 0;
}

.neirong {
    /*width: 704rpx;*/
    height:358rpx;
    position: relative;
    z-index: 1;
    left: 0;
    top: 0;
    padding:22rpx 32rpx;
    display: flex;
    flex-direction:column;
    align-items: center;
    justify-content: space-between;
}

.store {
    width:100%;
    margin-bottom:36rpx;
    display: flex;
    justify-content: space-between;
}
.store:nth-child(2){
    margin-bottom:22rpx;
}
.store view {
    display: block;
    min-width:136rpx;
    font-size: 32rpx;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    color: rgba(255, 255, 255, 1);
    line-height: 44rpx;
}




.bianhao {
    width:100%;
    height: 124rpx;
    /* font-size: 88rpx; */
    text-align: center;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    color: rgba(255, 255, 255, 1);
    line-height: 124rpx;
}

.search_input {
    display: flex;
    width:640rpx;
    height: 72rpx;
    justify-content: space-between;
    align-items: center;
    position: relative;
    line-height: 72rpx;
}

.search_input image {
    width: 48rpx;
    height: 48rpx;
    /*margin-right:22rpx;*/
    border-radius: 50%;
}

.search_input input {
    /*position: relative;*/
    width: 478rpx;
    height: 72rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx;
    font-size: 28rpx;
    padding-left: 20rpx;
    opacity:.7;
}

.search_input .get {
    
    height: 40rpx;
    /*margin-right:10rpx;*/
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    color: #fff;
    line-height: 40rpx;
}

.img_search {
    position: absolute;
    margin-right:20rpx;
    right:0rpx;
    opacity:1;
    border-radius: 0 !important;

}

.sacn_code {
    text-align: center;
    width: 100%;
    height: 120rpx;
    margin: 0 auto;
    position: fixed;
    bottom: 120rpx;
}

.sacn_code image {
    /* margin: 33rpx 0 18rpx 0; */
    color: white;
    width: 72rpx;
    height: 72rpx;
    padding: 24rpx;
    background: linear-gradient(180deg, rgba(71, 194, 248, 1) 0%, rgba(34, 143, 239, 1) 100%);
    border-radius: 50%;
}

.sacn_code view {
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    color: rgba(74, 74, 74, 1);
}

/* 主要内容 */

.record{
    padding-bottom: 300rpx;
}

.recent_ship {
    text-align: center;
    height: 78rpx;
    font-size: 32rpx;
    font-family: PingFang-SC-Medium;
    font-weight: normal;
    color: rgba(74, 74, 74, 1);
    line-height: 78rpx;
}

.shipid {
    display: flex;
    width: 640rpx;
    height: 134rpx;
    margin: 0 auto;
    font-size: 24rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 1px 0rpx 0px rgba(0, 0, 0, 0.13), 0px 1px 4rpx 1px rgba(0, 0, 0, 0.13);
    margin-top: 12rpx;
    padding: 16rpx 20rpx 32rpx 26rpx;
    border-radius: 4rpx;
    justify-content: flex-start;
    /* align-items: center; */
}


.img {
    width: 86rpx;
    height: 86rpx;
}

.img image {
  display:inline-block;
  line-height:86rpx;
    border-radius: 50%;
}

.ship_list {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex:1;
    margin-left:30rpx;
}

.ship_list .name {
    height: 50rpx;
    font-size: 28rpx;
    font-family: Helvetica;
    color: rgba(74, 74, 74, 1);
    line-height: 50rpx;
    display: flex;
    justify-content: space-between;
}

.ship_list .time {
    display: flex;
    justify-content: space-between;
    height: 50rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(155, 155, 155, 1);
    line-height: 50rpx;
}

.ship_num {
    font-size: 24rpx;
    font-family: SFUIDisplay-Medium;
    font-weight: 500;
    color: rgba(155, 155, 155, 1);
}
