.tabbar-title {
    font-size: 20rpx;
}
.circle {
    position: absolute;
    bottom: 6rpx;
    left: 50%;
    z-index: 10;
    transform: translate(-50%, 0);
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    
    /* box-shadow: 0px -1px 2px -5px rgba(0, 0, 0, 0.17); */
}

.just image {
    width: 50rpx;
    height: 50rpx;
}

.circle image {
    width: 100%;
    height: 100%;
}
