.just {
    display: flex;
    position: fixed;
    z-index: 10;
    bottom: 0;
    left: 0;
    width: 750rpx;
    height: 108rpx;
    /* padding: 0 12rpx; */
    background: #fff;
    /* box-sizing: border-box; */
    justify-content: space-between;
    align-items: center;
    box-shadow: 0px -1px 0px 0px rgba(215, 215, 215, 1);
}

.just > view {
    color: #9B9B9B;
    text-align: center;
    flex:1;
    
}
