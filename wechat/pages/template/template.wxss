.inner {
    height: 100%;
    width: 100%;
    background: rgba(74, 74, 74, 0.371);
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 99;
    animation: donghuabg 0.3s linear both;
}

@keyframes donghuabg {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

.inner1 {
    height: 100%;
