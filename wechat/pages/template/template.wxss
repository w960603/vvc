.wfr-content {
    width: 100%;
    min-height: 1034rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 22rpx 22rpx 0px 0px;
    position: absolute;
    bottom: 0;
    z-index: 9;
    animation: content 0.3s linear both;
}

@keyframes content {
    0% {
        bottom: -1500rpx;
    }

    100% {
        bottom: 0;
    }
}

